# https://edabit.com/challenge/v2Xk5fXkBrYq7dA83
# Create a function that inverts the rgb values of a given tuple.

proc invert_rgb(r, g, b : int) -> int, int, int {
	255 b -
	255 g -
	255 r -
}

proc print_rgb(b, g, r : int) -> void {
	r println drop
	g println drop
	b println drop
}

proc main() -> void {
	| 165 170 221 | !invert_rgb |!3| !print_rgb
}